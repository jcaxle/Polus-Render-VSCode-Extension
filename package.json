{
  "name": "polus-render",
  "displayName": "polus-render",
  "description": "Embed Polus Render into VSCode",
  "icon": "images/icon.png",
  "publisher": "jcaxle",
  "version": "0.0.1",
  "keywords": [
    "polus",
    "render",
    "zarr",
    "ome.tif",
    "science"
  ],
  "preview": true,
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/jcaxle/Polus-Render-VSCode-Extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcaxle/Polus-Render-VSCode-Extension"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Data Science",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Polus-Render",
      "properties": {
        "prompt.overlay.disable": {
          "description": "Displays a prompt asking the user to choose an overlay with \"open with Render\" context menu option.",
          "type": "boolean",
          "default": "True"
        },
        "prompt.polus.type.disable": {
          "description": "Displays a prompt asking the user to choose to use either online or local build of Render with \"open with Render\" context menu option.",
          "type": "boolean",
          "default": "True"
        },
        "prompt.default.local": {
          "description": "Use local build of Render by default.",
          "type": "boolean",
          "default": "True"
        }
      }
    },
    "menus": {
      "editor/title": [
        {
          "command": "polus-render.openPolusRender",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "resourcePath =~ /.*\\.zarr.*/",
          "command": "polus-render.openZarr",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .tif",
          "command": "polus-render.openTif",
          "group": "navigation"
        },
        {
          "command": "polus-render.openPolusRender",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "polus-render.openPolusRender",
        "title": "Open Polus Render"
      },
      {
        "command": "polus-render.openZarr",
        "title": "Open .zarr with Polus Render"
      },
      {
        "command": "polus-render.openTif",
        "title": "Open .ome.tif with Polus Render"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/serve-handler": "^6.1.4",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "get-port": "^5.1.1",
    "serve": "^14.2.1",
    "tsc": "^2.0.4"
  }
}
